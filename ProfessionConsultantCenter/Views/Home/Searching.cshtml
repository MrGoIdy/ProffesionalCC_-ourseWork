<div>
    <form class="navbar-form navbar-left" role="search" action="/Home/ListOfConsultations" method="post">
        <div class="form-group">
            <input type="text" class="form-control" onkeydown="SendPost()" id="Search" name="Search" placeholder="Search">
        </div>
        <button type="submit" class="btn btn-default">Отправить</button>
    </form>
</div>
<div class="list" id="list">
    @Html.Action("ListOfConsultation");
   
</div>



<script type="text/javascript">

    function SendPost() {
       
        $.ajax({
        type:'post',   
        url: 'ListOfConsultation',
        data: { 'search': document.getElementById('Search').value },
        response: 'text',
        success:function (data) {
            document.getElementById('list').innerHTML = data
       
        }
       
    });
    
    }

</script>